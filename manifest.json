{
    "manifest_version": 3,
    "key": "",
    "name": "__MSG_appName__",
    "version": "0.1.0",
    "default_locale": "en",
    "description": "__MSG_appDescription__",
    "author": "June07 <667@june07.com> (https://june07.com/)",
    "commands": {
        "rect-capture": {
            "suggested_key": {
                "default": "Alt+S"
            },
            "description": "__MSG_keyAltS__"
        },
        "page-capture": {
            "suggested_key": {
                "default": "Alt+X"
            },
            "description": "__MSG_keyAltX__"
        }
    },
    "action": {
        "default_icon": {
            "16": "/icon/icon16.png",
            "24": "/icon/icon24.png",
            "32": "/icon/icon32.png"
        },
        "ughed": {
            "16": "/icon/ughed/icon16.png",
            "24": "/icon/ughed/icon24.png",
            "32": "/icon/ughed/icon32.png"
        },
        "default_title": "__MSG_appName__"
    },
    "icons": {
        "16": "/icon/icon16.png",
        "24": "/icon/icon24.png",
        "32": "/icon/icon32.png",
        "48": "/icon/icon48.png",
        "64": "/icon/icon64.png",
        "96": "/icon/icon96.png",
        "128": "/icon/icon128.png",
        "256": "/icon/icon256.png",
        "384": "/icon/icon384.png",
        "512": "/icon/icon512.png"
    },
    "background": {
        "service_worker": "/src/sw.js"
    },
    "permissions": [
        "cookies",
        "declarativeNetRequestWithHostAccess",
        "declarativeNetRequestFeedback",
        "desktopCapture",
        "notifications",
        "scripting",
        "storage",
        "tabs",
        "webNavigation"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "externally_connectable": {
        "matches": [
            "*://*.june07.com/*"
        ]
    },
    "short_name": "__MSG_appShortName__",
    "version_name": "0.0.0",
    "declarative_net_request": {
        "rule_resources": [
            {
                "id": "interceptRuleAll",
                "enabled": false,
                "path": "rules/interceptRuleAll.json"
            },
            {
                "id": "interceptRuleInstagram",
                "enabled": true,
                "path": "rules/interceptRuleInstagram.json"
            }
        ]
    },
    "web_accessible_resources": [
        {
            "resources": [
                "/dist/index.html"
            ],
            "matches": [
                "https://*.june07.com/*"
            ]
        }
    ]
}